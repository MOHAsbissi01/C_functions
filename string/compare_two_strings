#include <stdio.h>
#include <string.h>

int main()
{
    char str1[100], str2[100];
    int result;

    printf("Enter the first string: ");
    scanf("%s", str1); // read the first string from the user

    printf("Enter the second string: ");
    scanf("%s", str2); // read the second string from the user

    result = strcmp(str1, str2); // compare the strings

    if (result == 0)
    {
        printf("The strings are equal.\n");
    }
    else if (result < 0)
    {
        printf("The first string is lexicographically smaller than the second string.\n");
    }
    else
    {
        printf("The first string is lexicographically greater than the second string.\n");
    }

    return 0;
}
