#include <stdio.h>

struct Person {
    char name[100];
    int age;
};

int main() {
    struct Person person;

    // fill the struct using scanf
    printf("Enter the name and age of the person: ");
    scanf("%s %d", person.name, &person.age);

    // print the struct
    printf("Name: %s, Age: %d\n", person.name, person.age);

    return 0;
}
